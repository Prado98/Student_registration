<%include header.ejs%>
<div class="container">
    <table>
        <thead>
          <tr>
              <th>Course Name</th>
              <th>Course Id</th>
              <th>Credits</th>
          </tr>
        </thead>
    
        <tbody>
        <%for(var i = 0; i< res1.length; i++){ %>
            <tr>
            <td><%=res1[i].name%></td>
            <td><%=res1[i].id%></td>
            <td><%=res1[i].credit%></td>
          </tr>
        <%}%>
        </tbody>
    </table>
</div>


</body>
